{
  "cli": {
    "version": ">= 9.0.10",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "development"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      },
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "staging-ios": {
      "distribution": "store",
      "channel": "staging",
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      },
      "ios": {
        "credentialsSource": "remote"
      }
    },
    "staging-android": {
      "distribution": "store",
      "channel": "staging",
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      },
      "ios": {
        "credentialsSource": "remote"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6748387089"
      },
      "android": {
        "track": "production"
      }
    },
    "preview": {},
    "staging-ios": {
      "ios": {
        "ascAppId": "6748387089"
      }
    },
    "staging-android": {
      "android": {
        "track": "internal"
      }
    }
  }
}
